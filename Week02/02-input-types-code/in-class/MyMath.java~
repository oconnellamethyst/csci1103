public class MyMath{
  public static void main(String args[]){
    double a = 4.5;
    double b = -4.1;
    double c = 0.5;
    double numerator   = -b + Math.sqrt(Math.pow(b,2) - 4*a*c);
//    double numerator   = -b + Math.sqrt(b*b - 4*a*c);
    double denominator = 2*a;
    double x = numerator / denominator;
    System.out.println(x);

  }
}